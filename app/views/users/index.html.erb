<div class="panel">
<p class="panel-heading">Users</p>
    <% @users.each do |user| %>
        <div class="panel-block">
        
            
            <article class="media">
                <div class="media-left">
                    <figure class="image is-32x32">
                        <img src="https://bulma.io/images/placeholders/64x64.png">
                    </figure>
                </div>
            
                <div class="panel-block">
                <div class="media-content">
                    <div class="content">
                    <%= link_to user_path(user) do%>
                        <p>
                            <%= user.username %>
        
                        </p>	
                    </div>
                </div>
                <% end %>
            </article>
        </div>
        
    <% end %>
</div>